@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/css/base.css" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/themes/icon.css?v=2.3" rel="stylesheet" />
    <link href="~/Content/css/loadTips/loadTips.css?v=2.3" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/easyui-rtl.css" rel="stylesheet" id="easyui-rtlcss" />
    <link href="~/Content/css/index.css?v=2.8" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Content/plug-in/layer/layer.min.js?v=1.14"></script>
    <script src="~/Content/plug-in/easyui/jquery.easyui.min.js"></script>
    <script src="~/Content/plug-in/jquery-validation/jquery.metadata.js"></script>
    <script src="~/Content/plug-in/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Content/plug-in/jquery-validation/messages_cn.js"></script>
    <script src="~/Content/plug-in/easyui/locale/easyui-lang-zh_CN.js" charset="utf-8"></script>
    <script src="~/Scripts/Base.js?v=1.07"></script>
    <script src="~/Content/plug-in/js/validataboxextend.js?v=1.0.5"></script>
    <script src="~/Content/plug-in/easyui/easyui-rtl.js" id="easyui-rtljs"></script>
    <title></title>
    <script type="text/javascript">
        $(function () {
            $("a[dir='search']").click(function () {
                $("#IsSearch").val("1");
            });
            $(".tabs li").click(function () {
                $("#IsSearch").val("0");
            });
            if (!!window.ActiveXObject || "ActiveXObject" in window) { //如果是IE
                //调用垃圾回收，强制清楚iframe内存。
                CollectGarbage();
                $("input[type='text']").click(function () {
                    $(this).focus();
                });
            }
            
        });
    </script>
</head>
<body>
    <div>
        <input type="hidden" id="IsSearch" name="IsSearch" value="0" />
        @RenderBody()
    </div>
</body>
</html>


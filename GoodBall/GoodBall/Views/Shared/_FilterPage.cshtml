@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/css/base.css" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/themes/icon.css?v=2.3" rel="stylesheet" />
    <link href="~/Content/css/loadTips/loadTips.css?v=2.3" rel="stylesheet" />
    <link href="~/Content/plug-in/easyui/easyui-rtl.css" rel="stylesheet" id="easyui-rtlcss" />
    <link href="~/Content/plug-in/jquery-date-range-picker/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/css/index.css?v=2.8" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Content/plug-in/layer/layer.min.js?v=1.14"></script>
    <script src="~/Content/plug-in/easyui/jquery.easyui.min.js"></script>
    <script src="~/Content/plug-in/jquery-validation/jquery.metadata.js"></script>
    <script src="~/Content/plug-in/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Content/plug-in/jquery-validation/messages_cn.js"></script>
    <script src="~/Content/plug-in/easyui/locale/easyui-lang-zh_CN.js" charset="utf-8"></script>
    <script src="~/Scripts/Base.js?v=1.07"></script>
    <script src="~/Content/plug-in/js/validataboxextend.js?v=1.0.3"></script>
    <script src="~/Content/plug-in/easyui/easyui-rtl.js" id="easyui-rtljs"></script>
    <script src="~/Content/plug-in/js/jquery.placeholder.js"></script>
    <script src="~/Scripts/datectlformatter.js"></script>
    <script src="~/Content/plug-in/jquery-date-range-picker/moment.min.js"></script>
    <script src="~/Content/plug-in/jquery-date-range-picker/jquery.daterangepicker.js"></script>
    <title></title>
    <script type="text/javascript">
        $(function () {

            $("a[dir='search']").click(function () {
                $("#IsSearch").val("1");
            });

            $(".tabs li").click(function () {
                $("#IsSearch").val("0");
            });

            $(".advancedQuery").click(function () {
                $(this).parents(".easyui-filter.filter").siblings(".advancedQueryWrap").toggle();
            });

            if (!!window.ActiveXObject || "ActiveXObject" in window) { //如果是IE
                //调用垃圾回收，强制清楚iframe内存。
                CollectGarbage();
            }
        });

        function initdateRangePicker(option) {
            var _$option = $.extend(option);
            $('#' + _$option.id).dateRangePicker({
                language: 'cn',
                startOfWeek: 'monday',
                separator: ' 至 ',
                format: 'YYYY-MM-DD',
                singleMonth: false,
                autoClose: false
            }).bind('datepicker-apply', function (event, obj) {
                var str1 = "", str2 = "",str3="";
                if (obj.date1 != "" && obj.date2 != "" && obj.date2 != "Invalid Date") {
                    str1 = myformatter(new Date(obj.date1));
                    str2 = myformatter(new Date(obj.date2));
                    str3 = obj.value;
                $('#' + event.currentTarget.id).val(str3);
                    $('#' + event.currentTarget.id + _$option.start).attr("value", str1);
                    $('#' + event.currentTarget.id + _$option.end).attr("value", str2);
                }

            }).bind('datepicker-apply-clear', function (event, obj) {
                if (obj.value == "") {
                    $('#' + event.currentTarget.id + _$option.start).attr("value", "");
                    $('#' + event.currentTarget.id + _$option.end).attr("value", "");
                }
                $('#' + event.currentTarget.id).val("");
            });
        }
        var _hmt = _hmt || [];
        (function () {
            if (window.location.hostname == "pep.yunfangdata.com") {
                var hm = document.createElement("script");
                hm.src = "https://hm.baidu.com/hm.js?34629db944cb5fb793c52ca4e31bc41e";
                var s = document.getElementsByTagName("script")[0];
                s.parentNode.insertBefore(hm, s);
            }
        })();
    </script>
</head>
<body>
    <div>
        <input type="hidden" id="IsSearch" name="IsSearch" value="0" />
        @RenderBody()
    </div>
</body>
</html>

